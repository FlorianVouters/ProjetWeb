<!DOCTYPE html>
<html lang="fr">
<head>
    {% block head %}


        <meta charset="UTF-8">
        <meta name="viewport"
              content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
        <meta http-equiv="X-UA-Compatible" content="ie=edge">
        <meta name="description" content="Site officiel du BDE du CESI">
        <meta name="keywords" content="cesi-bde, bde, cesi, aix-en-provence, angoulême, arras, bordeaux, brest, dijon, grenoble, la rochelle, le mans, lille, lyon, montpellier, nancy, nantes, nice, orléans, paris, nanterre, pau, reims, rouen, saint-nazaire, strasbourg, toulouse">
        <link rel="icon" type="image/png" href="img/bdeIcon.ico">
        <link href="https://maxcdn.bootstrapcdn.com/font-awesome/4.4.0/css/font-awesome.min.css" rel="stylesheet">
        <link rel="stylesheet" href="https://stackpath.bootstrapcdn.com/bootstrap/4.1.0/css/bootstrap.min.css" integrity="sha384-9gVQ4dYFwwWSjIDZnLEWnxCjeSWFphJiwGPXr1jddIhOegiu1FwO5qRGvFXOdJZ4" crossorigin="anonymous">
        <link rel="stylesheet" href="{{ asset('build/css/style.css') }}" type="text/css">


        <title>{% block title %}BDE CESI{% endblock %}</title>



    {%  endblock %}
</head>

<body>


    {% block navbarNetwork %}
    <!-- Info & Networks -->
    <header>
        <nav class="navbar navbar-expand-sm sticky-top navbar-dark bg-brown">
            <a class="navbar-brand" href="#"></a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarNetwork" aria-controls="navbarNetwork" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse offset-md-2" id="navbarNetwork">
                <ul class="navbar-nav mr-auto mt-2 mt-lg-0">
                    <li class="nav-item">
                        <a class="nav-link" href="#" style="color: white">Inscription</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#" style="color: white">Connexion</a>
                    </li>
                </ul>
                <ul class="navbar-nav mr-auto mt-2 mt-lg-0 offset-md-3">
                    <li class="nav-item">
                        <a class="nav-link" href="#" style="color: white"><img src="img/facebook.png" width="30" height="30"></a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#" style="color: white"><img src="/img/twitter.png" width="30" height="30"></a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#" style="color: white"><img src="img/youtube.png" width="30" height="30"></a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#" style="color: white"><img src="img/at.png" width="30" height="30"></a>
                    </li>
                </ul>
            </div>
        </nav>
    </header>
    {% endblock %}


    {% block navbarWebsite %}

        <!-- NavBar WEBSITE -->
        <nav class="navbar navbar-expand-md sticky-top navbar-light bg-light">
            <a class="navbar-brand" href="#"><img src="img/bde-tls.jpg" width="100" height="100" class="d-inline-block align-center" alt="Logo BDE"><strong> BDE CESI</strong></a>
            <button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarWebsite" aria-controls="navbarWebsite" aria-expanded="false" aria-label="Toggle navigation">
                <span class="navbar-toggler-icon"></span>
            </button>
            <div class="collapse navbar-collapse" id="navbarWebsite">
                <ul class="navbar-nav mr-auto mt-2 mt-lg-0">
                    <li class="nav-item">
                        <a class="nav-link" href="#{#{{ path('index') }}#}">Accueil</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#">Evenement</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#">Boite a idees</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#">Boutique</a>
                    </li>
                    <li class="nav-item">
                        <a class="nav-link" href="#">Contact</a>
                    </li>
                </ul>
            </div>
        </nav>

    {% endblock %}



    {% block body %}

        {# CODE HERE #}

    {% endblock %}



    <footer>
        {% block navbarCesi %}

            <!-- NavBar WEBSITES CESI -->
            <nav class="navbar navbar-expand-xl navbar-white bg-white">
            <!--<button class="navbar-toggler" type="button" data-toggle="collapse" data-target="#navbarTogglerDemo02" aria-controls="" aria-expanded="false" aria-label="Toggle navigation">-->
            <!--<span class="navbar-toggler-icon"></span>-->
            <!--</button>-->
            <div class="collapse navbar-collapse offset-md-2">
                <ul class="navbar-nav mr-auto mt-2 mt-lg-0">
                    <li class="nav-link">
                        <a class="nav-link" href="#"><img src="img/logo-footer-cesi.png" alt="Cesi logo"></a>
                    </li>
                    <li class="nav-link">
                        <a class="nav-link" href="#"><img src="img/logo-footer-cesi-exia.png" alt="Exia logo"></a>
                    </li>
                    <li class="nav-link">
                        <a class="nav-link" href="#"><img src="img/logo-footer-cesi-eice.png" alt="EI logo"></a>
                    </li>
                    <li class="nav-link">
                        <a class="nav-link" href="#"><img src="img/logo-footer-cesi-alternance.png" alt="Alter logo"></a>
                    </li>
                    <li class="nav-link">
                        <a class="nav-link" href="#"><img src="img/logo-footer-cesi-enterprise.png" alt="Enterp logo"></a>
                    </li>
                    <li class="nav-link">
                        <a class="nav-link" href="#"><img src="img/logo-footer-cesi-certification.png" alt="Certif logo"></a>
                    </li>
                    <li class="nav-link">
                        <a class="nav-link" href="#"><img src="img/logo-footer-cesi-alumni.png" alt="Alumni logo"></a>
                    </li>
                </ul>
            </div>
        </nav>

        {% endblock %}
        {% block legals %}

            <!-- Legal Mentions -->
            <div class="text-center mentions" style="background: white">
                <a href="#">Mentions légales</a>
            </div>

        {% endblock %}
    </footer>

        <script src="https://cdnjs.cloudflare.com/ajax/libs/jquery/3.3.1/jquery.js"></script>
        <script src="{{  asset('build/js/bootstrapBundle.js') }}"></script>
        <script src="{{  asset('build/js/buttonTrigger.js') }}"></script>
        {#<script src="{{ url("https://cdnjs.cloudflare.com/ajax/libs/vue/2.5.16/vue.js") }}"></script>   <!-- Version chargée sur le net -->#}
        {#<script src="{{ path("https://cdnjs.cloudflare.com/ajax/libs/popper.js/1.14.0/umd/popper.min.js") }}" integrity="sha384-cs/chFZiN24E4KMATLdqdvsezGxaGsi4hLGOzlXwp5UZB1LY//20VyM2taTB4QvJ" crossorigin="anonymous"></script>#}
        {#<script src="{{ path("https://stackpath.bootstrapcdn.com/bootstrap/4.1.0/js/bootstrap.min.js") }}" integrity="sha384-uefMccjFJAIv6A+rW+L4AHf99KvxDjWSu1z9VI8SKNVmz4sk7buKt/6v9KI65qnm" crossorigin="anonymous"></script>#}
        {% block scripts %}
        {% endblock %}



    </body>
</html>
